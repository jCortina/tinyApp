<!DOCTYPE html>
<html lang="en">
<head>
  <title>List of URLs</title>
</head>
<body>
  <%- include partials/_header.ejs %> 
  <!-- List of URL's -->
  <h4>Here are the List of URL's</h4>
    <ul>
      <% for (let url in urls) { %>
        <form method="POST" action="/urls/<%= url %>/delete"> 
          <li>
            <%= url %> -> 
            <a href=<%= urls[url] %>>
            <%= urls[url] %>
            </a>
            <a href="<%= root %>urls/<%= url %>/update">Edit URL</a>
            <input type="submit" name="delete">
          </li>
        </form>
     <% } %>
    </ul>
</body>
</html>
